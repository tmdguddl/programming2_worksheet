#include <stdio.h>

int main() 
{
   //enum = 새로운 변수형 -> 정수로 동작
   enum Fruit {Apple, Banana, Orange};

   Fruit fruit;

   printf("과일 선택(Apple: 0, Banana: 1, Orange: 2): ");
   scanf_s("%d", &fruit);

   if (fruit == Apple)
      printf("Apple\n");
   else if (fruit == Banana)
      printf("Banana\n");
   else if (fruit == Orange)
      printf("Orange\n");
}
